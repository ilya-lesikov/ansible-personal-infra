http:
  routers:
    api:
      entryPoints:
        - api
      rule: PathPrefix(`/api`) || PathPrefix(`/dashboard`)
      service: api@internal
      middlewares:
        - api_auth

  middlewares:
    api_auth:
      digestAuth:
        users:
          - "admin:traefik:bfd5f4c74bc7047108bc3f6c91b3f4fa"
